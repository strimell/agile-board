{
  "extends": [
    "react-app",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "no-unexpected-multiline": "error",
    "quotes": ["error", "double", { "avoidEscape": true, "allowTemplateLiterals": true }],
    "keyword-spacing": ["error", { "before": true, "after": true }],
    "prettier/prettier": "warn"
  }
}